{
  "name": "deepseek-v3.2",
  "provider": "nvidia",
  "model_id": "deepseek-ai/deepseek-r1",
  "api_endpoint": "https://integrate.api.nvidia.com/v1/chat/completions",
  "description": "DeepSeek V3.2 - Advanced reasoning model with strong performance on complex tasks",
  "headers": {
    "Authorization": "Bearer $NVIDIA_API_KEY",
    "Accept": "application/json",
    "Content-Type": "application/json"
  },
  "parameters": {
    "temperature": 0.6,
    "top_p": 0.7,
    "frequency_penalty": 0,
    "presence_penalty": 0,
    "max_tokens": 4096,
    "stream": true
  },
  "capabilities": [
    "reasoning",
    "code-generation",
    "analysis",
    "math"
  ],
  "context_length": 65536,
  "notes": "Requires NVIDIA_API_KEY environment variable to be set"
}
