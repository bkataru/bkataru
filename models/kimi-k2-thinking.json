{
  "name": "kimi-k2-thinking",
  "provider": "nvidia",
  "api_endpoint": "https://integrate.api.nvidia.com/v1/chat/completions",
  "model_id": "moonshotai/kimi-k2-instruct",
  "description": "Kimi K2 is a powerful reasoning model with strong thinking capabilities, optimized for complex problem-solving tasks.",
  "headers": {
    "Authorization": "Bearer $NVIDIA_API_KEY",
    "Accept": "application/json",
    "Content-Type": "application/json"
  },
  "parameters": {
    "temperature": 0.6,
    "top_p": 0.95,
    "frequency_penalty": 0,
    "presence_penalty": 0,
    "max_tokens": 4096,
    "stream": true
  },
  "features": {
    "thinking": true,
    "function_calling": true,
    "long_context": true
  },
  "context_length": 131072,
  "notes": "Requires NVIDIA_API_KEY environment variable. Best suited for reasoning and multi-step problem solving."
}
